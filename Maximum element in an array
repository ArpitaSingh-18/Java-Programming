import java.util.Scanner;
public class Max_elementin_an_array {

	public static void main(String[] args) {
		
		Scanner sr=new Scanner(System.in);
		int []arr=new int[10];
		int max=0,i;
		System.out.println("Enter the elements of the array\n");
		for(i=0;i<arr.length;i++)
		{
			arr[i]=sr.nextInt();
		}
		max=arr[0];
		for(i=0;i<arr.length;i++)
		{
			if(max<arr[i])
				max=arr[i];
		}
		System.out.println("\nThe maximum number of the given array is "+max);

	}

}
